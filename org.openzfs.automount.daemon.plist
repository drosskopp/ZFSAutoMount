<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.openzfs.automount.daemon</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/ZFSAutoMount.app/Contents/MacOS/ZFSAutoMount</string>
		<string>--boot-mount</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<false/>
	<key>StandardOutPath</key>
	<string>/var/log/zfs-automount.log</string>
	<key>StandardErrorPath</key>
	<string>/var/log/zfs-automount-error.log</string>
</dict>
</plist>
